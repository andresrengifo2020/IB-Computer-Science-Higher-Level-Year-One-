public class FiremanKarel extends Karel
{
    public void run()
    {
        move();
        turnLeft();
        move();
        move();
        move();
        turnRight();
        move();
        turnRight();
        move();
        move();
        move();
        turnLeft();
    }
    
    public void turnRight()
    {
        
        turnLeft();
        turnLeft();
        turnLeft();
    }
}
