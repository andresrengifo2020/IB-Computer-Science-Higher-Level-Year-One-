public class HurdlesKarel extends SuperKarel
{
    public void run()
    {
        // your code here 
        
        for (int i = 0; i< 5; i++)
        {
        move();
        move();
        jumpHurdle();
        }
    }


    private void jumpHurdle()
    {
        
      turnLeft();
      move();
      turnRight();
      move();
      turnRight();
      move();
      turnLeft();
        
    }
    
}
