public class TowerKarel extends Karel
{
    public void run()
    {
        move();
    	putBall();
    	turnLeft();
    	move();
    	putBall();
    	move();
    	putBall();
    	move();
    	turnRight();
    }
    
    private void turnRight()
    {
        turnLeft();
        turnLeft();
        turnLeft();
    }
}
