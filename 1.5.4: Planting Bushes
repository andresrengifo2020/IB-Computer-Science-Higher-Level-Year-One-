public class KarelProgram extends Karel
{
    public void run()
    {
        putInPosition();
        makeBush();
        putInPosition();
        makeBush();
        move();
        
    }
    
    public void makeBush()
    {
        putBall();
        
        move();
        putBall();
        turnLeft();
        move();
        putBall();
        turnLeft();
        move();
        putBall();
        turnLeft();
        move();
        turnLeft();
        move();
        
        
        
    }
    
    public void putInPosition()
    {
        
        move();
        move();
        move();
    }
}
